{
  "config": {
    "default": true,
    "MD013": {
      "line_length": 120,
      "code_blocks": false,
      "tables": false
    },
    "MD033": {
      "allowed_elements": ["sub", "sup", "kbd", "mark", "span", "details", "summary"]
    },
    "MD041": false,
    "MD046": {
      "style": "fenced"
    }
  },
  "ignores": [
    "node_modules/**",
    "bin/**",
    "obj/**",
    "**/*.csproj"
  ]
}